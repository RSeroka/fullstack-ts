{
  "name": "fullstack-ts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/express/server.js",
  "scripts": {
    "all-clean": "rimraf ./dist",
    "server-build": "tsc",
    "server-test": "tsc && env-cmd -e mocha-test mocha \"dist/**/*.js\"",
    "client-build": "cd client && npm run build",
    "all-rebuild": "npm run all-clean && concurrently -c auto \"npm:server-build\" \"npm:client-build\"",
    "local-express-server": "node ./dist/src/express/server.js",
    "docker-build": "docker build -t rseroka/sudoku-ts .",
    "docker-run": "docker run --publish 80:8080 --restart on-failure:5 --name sudoku-container rseroka/sudoku-ts",
    "postinstall": "ts-patch install"
  },
  "author": "rseroka",
  "license": "UNLICENCED",
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.3",
    "@tsconfig/strictest": "^2.0.1",
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@types/sinon": "^10.0.13",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "concurrently": "^8.0.1",
    "create-react-app": "^5.0.1",
    "env-cmd": "^10.1.0",
    "mocha": "^10.2.0",
    "rimraf": "^4.1.2",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "typia": "^4.2.1",
    "typescript": "^5.2.2",
    "ts-patch": "^3.0.2"
  }
}